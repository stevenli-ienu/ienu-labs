{
    "name": "refactoring-pocs",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/refactoring-pocs/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/js:swc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/libs/refactoring-pocs",
                "main": "libs/refactoring-pocs/src/index.ts",
                "tsConfig": "libs/refactoring-pocs/tsconfig.lib.json",
                "assets": [
                    "libs/refactoring-pocs/*.md"
                ]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/refactoring-pocs/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": [
                "{options.outputFile}"
            ]
        }
    }
}